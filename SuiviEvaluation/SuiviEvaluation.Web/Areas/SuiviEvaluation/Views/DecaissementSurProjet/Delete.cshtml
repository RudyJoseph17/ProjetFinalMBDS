@model SuiviEvaluation.Application.Dtos.DecaissementSurProjetDto
@{
    ViewData["Title"] = "Supprimer décaissement";
}

<h1>@ViewData["Title"]</h1>

<h3>Voulez-vous vraiment supprimer ce décaissement ?</h3>

<dl class="row">
    <dt class="col-sm-2">Projet</dt>
    <dd class="col-sm-10">@Model.IdIdentificationProjet</dd>

    <dt class="col-sm-2">Activité</dt>
    <dd class="col-sm-10">@Model.IdActivites</dd>

    <dt class="col-sm-2">Exercice début</dt>
    <dd class="col-sm-10">@Model.ExerciceFiscalDebut</dd>

    <dt class="col-sm-2">Exercice fin</dt>
    <dd class="col-sm-10">@Model.ExerciceFiscalFin</dd>

    <dt class="col-sm-2">Mois</dt>
    <dd class="col-sm-10">@Model.MoisDecaissement</dd>

    <dt class="col-sm-2">Montant</dt>
    <dd class="col-sm-10">@Model.MontantDecaissement</dd>
</dl>

<form asp-action="Delete" asp-route-id="@Model.IdActivites" method="post">
    @Html.AntiForgeryToken()
    <button type="submit" class="btn btn-danger">Oui, supprimer</button>
    <a asp-action="Index" class="btn btn-secondary">Annuler</a>
</form>

@model BanqueProjet.Web.Models.GrilleDDPViewModel
<div id="grille-form-container" class="card mb-3" style="display:none;">
    <div class="card-header">Grille DDP - Analyse</div>
    <div class="card-body">
        <form asp-action="Analyse" method="post">
            <input type="hidden" asp-for="GDdpIdentificationProjets.IdIdentificationProjet" />

            <div class="mb-3">
                <h5>Identification</h5>
                <partial name="_Identification" model="Model.GDdpIdentificationProjets" />
            </div>
            <div class="mb-3">
                <h5>Stratégie</h5>
                <partial name="_Strategie" model="Model.GDdpStrategieGestion" />
            </div>
            <!-- répéter pour Resume, Etudes, Calendriers, AspectsLegaux, Commentaires, Avis -->

            <button type="submit" class="btn btn-success">Enregistrer l’analyse</button>
        </form>
    </div>
</div>

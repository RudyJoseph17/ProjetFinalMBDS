@model BanqueProjet.Web.Models.InstitutionSectorielleViewModel
@{
    ViewData["Title"] = "Créer une Institution Sectorielle";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="IdInstitutionSectorielle"></label>
        <input asp-for="IdInstitutionSectorielle" class="form-control" />
        <span asp-validation-for="IdInstitutionSectorielle" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="NomInstitutionSectorielle"></label>
        <input asp-for="NomInstitutionSectorielle" class="form-control" />
        <span asp-validation-for="NomInstitutionSectorielle" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="MissionInstitutionSectorielle"></label>
        <textarea asp-for="MissionInstitutionSectorielle" class="form-control"></textarea>
        <span asp-validation-for="MissionInstitutionSectorielle" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="AttributionsInstitutionSectorielle"></label>
        <textarea asp-for="AttributionsInstitutionSectorielle" class="form-control"></textarea>
        <span asp-validation-for="AttributionsInstitutionSectorielle" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-success">Créer</button>
    <a asp-action="Index" class="btn btn-secondary">Annuler</a>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}


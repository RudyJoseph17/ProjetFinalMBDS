@model BanqueProjet.Application.Dtos.IdentificationProjetDto

@{
    ViewData["Title"] = "Modifier Identification Projet Plat";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="IdIdentificationProjet" />

    <div class="form-group">
        <label asp-for="IdIdentificationProjet" class="control-label"></label>
        <input asp-for="IdIdentificationProjet" class="form-control" />
        <span asp-validation-for="IdIdentificationProjet" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Modifier</button>
    <a asp-action="Edit" asp-route-id="@Model.IdIdentificationProjet" class="btn btn-secondary">Annuler</a>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}


@model BanqueProjet.Application.Dtos.IdentificationProjetDto

@{
    ViewData["Title"] = "Supprimer Identification Projet Plat";
}

<h2>@ViewData["Title"]</h2>

<h4>Voulez-vous vraiment supprimer cet enregistrement ?</h4>
<dl class="row">
    <dt class="col-sm-3">Id Projet</dt>
    <dd class="col-sm-9">@Model.IdIdentificationProjet</dd>
</dl>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="IdIdentificationProjet" />
    <button type="submit" class="btn btn-danger">Supprimer</button>
    <a asp-action="Delete" asp-route-id="@Model.IdIdentificationProjet" class="btn btn-secondary">Annuler</a>
</form>


@model Programmation.Application.Dtos.ProgrammationProjetDto

@{
    ViewData["Title"] = "Supprimer la programmation";
}

<h2>@ViewData["Title"]</h2>

<h3>Voulez-vous vraiment supprimer ce projet ?</h3>

<div>
    <strong>Id projet :</strong> @Model.IdIdentificationProjet
</div>

<form asp-action="DeleteProgrammation" method="post">
    <input type="hidden" asp-for="IdIdentificationProjet" />
    <button type="submit" class="btn btn-danger">Supprimer</button>
    <a asp-action="IndexProgrammations" class="btn btn-secondary">Annuler</a>
</form>

